{
  "name": "jeandormehl/laracache",
  "license": "MIT",
  "description": "InterSystems Cache support for Laravel 5 using ODBC",
  "keywords": [
    "laravel5",
    "laravel",
    "intersystems",
    "cache",
    "pdo",
    "odbc"
  ],
  "homepage": "https://github.com/jeandormehl/laracache",
  "authors": [
    {
      "name": "Jean Dormehl",
      "email": "jdormehl@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "^5.0|^6.0",
    "illuminate/database": "^5.0|^6.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.13",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.0",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Laracache\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "v1.0-dev"
    },
    "laravel": {
      "providers": [
        "Laracache\\Cache\\ServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
